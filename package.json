{
  "name": "compare-san",
  "version": "1.0.0",
  "description": "比較さんを使えば、ディスプレイモニターやノートパソコンといった家電を表で比較できます。自分なりの好みや要望に合わせて評価をつけることができ、その合計点で最も自分に合った家電を決断できます。迷うことは有りません。",
  "author": "Texmeijin",
  "private": true,
  "scripts": {
    "dev": "cp .env.dev .env; nuxt --port 3333",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "cp .env.prod .env; nuxt generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/markdownit": "^1.2.7",
    "@nuxtjs/style-resources": "^1.0.0",
    "@toast-ui/vue-editor": "^1.1.1",
    "@toycode/markdown-it-class": "^1.2.1",
    "core-js": "^3.6.4",
    "dayjs": "^1.8.19",
    "eslint-plugin-vue": "^6.1.1",
    "firebase": "^8.3.1",
    "nuxt": "^2.15.3",
    "typescript": "^4.2.3",
    "vue-awesome": "^4.0.2",
    "vue-property-decorator": "^8.2.1",
    "vue-scrollto": "^2.17.1",
    "vue-simplemde": "^1.0.4",
    "vuex-class": "^0.3.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "^0.1.3",
    "@nuxtjs/tailwindcss": "^1.2.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.1.0",
    "eslint": "^6.7.1",
    "fibers": "^5.0.0",
    "node-sass": "^7.0.0",
    "prettier-eslint": "^9.0.1",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "vue-jest": "^3.0.7",
    "webpack-bundle-analyzer": "^3.6.0"
  }
